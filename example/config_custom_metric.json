{
    "hdl" : {
        "source" : "example/mult_2_bit.sv",
        "top"    : "mult_2_bit",
        "output" : "prova"
    },
    "als" : {
        "cache"    : "lut_catalog.db",
        "cut_size" : 4,
        "solver"   : "btor",
        "timeout"  : 60000
    },
    "error" : {
        "metric"       : {"module" : "metrics/relative_error_magnitude", "function" : "compute_rem", "reduce": "max"},
        "threshold"    : 16,
        "vectors"      : 10,
        "dataset"      : "example/dataset.csv",
        "weights"      : {
            "\\o[0]" : 1,
            "\\o[1]" : 2,
            "\\o[2]" : 4,
            "\\o[3]" : 8
        }

    },
    "hardware" : {
        "metrics" : ["gates"]
    },
    "amosa" : {
        "archive_hard_limit"       : 3,
        "archive_soft_limit"       : 5,
        "archive_gamma"            : 2,
        "hill_climbing_iterations" : 5,
        "initial_temperature"      : 5,
        "final_temperature"        : 0.01,
        "cooling_factor"           : 0.9,
        "annealing_iterations"     : 5,
        "annealing_strength"       : 1,
        "early_termination"        : 20
    }
}
